alias p='ps aux | grep'

alias res='shutdown -r now'
alias off='shutdown -h now'
alias sus='sudo systemctl suspend'
alias blank='xdg-screensaver activate'



### BOOTING *****************

alias safekrnl='sudo grub-reboot "5"; systemctl reboot'
alias lnx='sudo grub-reboot "6"; systemctl reboot'
alias lnxzen='sudo grub-reboot "0"; systemctl reboot'
alias lnxlts='sudo grub-reboot "3"; systemctl reboot'

alias krnls='pacman -Q linux | grep -P "^linux-image[^ ]*[0-9]\-amd64"'
alias kinst='ls -1 /boot/vmlinuz-*'
# alias windows='sudo grub-reboot 4; systemctl reboot'
alias edgrub='sudo micro /etc/default/grub'
alias mkgrub='sudo grub-mkconfig -o /boot/grub/grub.cfg'



